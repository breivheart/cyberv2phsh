{
    "manifest_version": 3,
    "name": "Phishing Detector",
    "version": "1.0",
    "description": "Evaluates web pages for phishing attempts.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "background": {
      "service_worker": "service-worker.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    }
  }
  